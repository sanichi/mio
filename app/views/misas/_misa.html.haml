%tr
  %td
    = link_to misa.title, misa, "data-turbo" => "false"
    - if misa.japanese
      = t("misa.abbrev.japanese")
  %td= misa.series
  %td.text-center= misa.number
  %td.text-center= misa.lines
  %td.text-center= misa.minutes
  %td.text-center= misa.published&.strftime("%y-%m-%d")
  %td.text-center
    - if misa.url
      = link_to t("misa.abbrev.url"), misa.url, target: t("misa.target")
    - if misa.alt
      = link_to t("misa.abbrev.alt"), misa.alt, target: t("misa.target")
