- content_for :head do
  = javascript_include_tag "https://www.google.com/jsapi"
- content_for :javascript do
  = render "graph.js", mgd: @mgd

= render "utils/title", key: "kg.data"

#chart{style: "height: #{@mgd.full_height}px"}

.text-center
  = link_to t("kg.index"), masses_path, class: "btn btn-primary"
  = link_to t("kg.new"), new_mass_path, class: "btn btn-primary"
