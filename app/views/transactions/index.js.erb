$("#results").html("<%= j render('results', transactions: @transactions) %>");

<% if @corrections[:date] %>
$("#date").val("<%= @corrections[:date]%>");
<% end %>

<% if @corrections[:days] %>
$("#days").val("<%= @corrections[:days]%>");
<% end %>

<% if @corrections[:upload_id] %>
$("#upload_id").val("<%= @corrections[:upload_id]%>");
<% end %>