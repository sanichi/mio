  -# Separators and indicators for when there is too much or too little space.
  %line.indicator{x1: "430", x2: "406", y1: "818", y2: "792"}
  %line.indicator{x1: "410", x2: "390", y1: "827", y2: "805"}
  %line.separator{x1: "106", x2: "128", y1: "1040", y2: "990"}

  -# City marks, links and texts. Cities with a similar name as their prefecture omit
  -# the text to save space and their markers are styled differently to indicate this.
  - elements.select{|p| p.city? || p.core? || p.designated? }.each do |p|
    - if p.mark_y
      %circle{"data-action" => "click->map#clicked", "data-map-place-param" => p.id, cx: p.mark_x, cy: p.mark_y, r: "6", class: p.name_echo? ? "ken" : "shi"}
    - if p.text_y
      %text.shi{x: p.text_x, y: p.text_y}= p.sjname

  -# Attraction marks (on their own without text as it's usually too cramped).
  - elements.select{|p| p.attraction? }.each do |p|
    - if p.mark_y
      %circle.atr{"data-action" => "click->map#clicked", "data-map-place-param" => p.id, cx: p.mark_x, cy: p.mark_y, r: "4"}

  -# Prefecture texts.
  - elements.select{|p| p.prefecture? }.each do |p|
    - if p.text_y
      %text.ken{x: p.text_x, y: p.text_y}= p.sjname
